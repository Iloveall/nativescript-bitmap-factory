{
  "name": "@atabix/nativescript-bitmap-factory",
  "description": "A NativeScript module for creating and manipulating bitmap images. Forked from whutsfree and updated for NS8.",
  "version": "3.0.0",
  "nativescript": {
    "platforms": {
      "ios": "8.0.0",
      "android": "8.0.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atabix/nativescript-bitmap-factory.git"
  },
  "keywords": [
    "NativeScript",
    "Bitmaps",
    "Factory",
    "Factories",
    "Images",
    "Circles",
    "Lines",
    "Args",
    "Rectangles",
    "Ovals",
    "Points",
    "Text",
    "Strings",
    "PNG",
    "JPEG",
    "JPG"
  ],
  "scripts": {
    "setup": "npm i && ts-patch install"
  },
  "compilerOptions": {
    "plugins": [
      {
        "transform": "@nativescript/webpack/transformers/ns-transform-native-classes",
        "type": "raw"
      }
    ]
  },
  "author": "Marcel Joachim Kloubert <marcel.kloubert@gmx.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atabix/nativescript-bitmap-factory/issues"
  },
  "homepage": "https://github.com/atabix/nativescript-bitmap-factory",
  "devDependencies": {
    "@nativescript/core": "~8.0.0",
    "@nativescript/android": "~8.0.0",
    "@nativescript/ios": "~8.0.0",
    "@nativescript/types": "~8.0.0",
    "typescript": "~4.1.3",
    "@types/node": "~14.14.13"
  },
  "main": "index",
  "typings": "index.d.ts"
}
